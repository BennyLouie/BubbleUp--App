<% if params[:id].to_i != @current_user.id %>

<p>
  <strong>Name:</strong>
  <%= @user.full_name %>
</p>

<p>
  <strong>Photo url:</strong>
  <%= @user.photo_url %>
</p>

<% else%>
  




<p id="notice"><%= notice %></p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Name:</strong>
  <%= @user.full_name %>
</p>

<p>
  <strong>Photo url:</strong>
  <%= @user.photo_url %>
</p>

<%= button_to 'Edit', edit_user_path(@user), method: :get %> 
<%= button_to 'Delete User', user_path(@user), method: :delete %>
<%= link_to 'View Boba Shops', shops_path%>
<%= button_to 'Log out', '/logout', method: :delete %> 

<%= link_to 'My Reviews', shop_reviews_path %>
<%= link_to 'My Recommendations', recommendations_path %>


<% end %> 